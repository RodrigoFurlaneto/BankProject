package entities;

abstract class  Account {

	private Double balance;
	private Double value;
	
	public Account() {
		
	}
	
	public Account(Double value) {
		this.value = value;
	}

	public Double getBalance() {
		return balance;
	}

	public Double getValue() {
		return value;
	}
	
	public void deposit(double value) {
		balance += value;
	}
}
